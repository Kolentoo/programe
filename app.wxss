.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


.bottom {position:fixed;background: #fff;border-top: 1px solid #e6e6e6;display: flex;justify-content: center;bottom:0;left:0;width:100%;height:40px;}
.bottom .bottom-list {font-size: 14px;color:#333;line-height: 40px;margin:0 auto;}
.bottom .bottom-on {color:#e09015;}

